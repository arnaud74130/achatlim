
<%= bootstrap_form_for(@etablissement) do |f| %>
    <%= render 'shared/model_errors', modele: @etablissement %>

    <%= f.text_field :nom %>
    <%= f.text_field :code_adherent %>
    <%= f.telephone_field :telephone %>
    <%= f.telephone_field :fax %>
    <%= f.text_field :adresse_ligne1 %>
    <%= f.text_field :adresse_ligne2 %>
    <%= f.text_field :code_postal %>
    <%= f.text_field :ville %>
    <%= f.submit nil, class: 'btn btn-primary' %>
   <!-- %= hf_cancel_submit(f, etablissements_path) % -->

<% end %>
