<%= render 'fixed_elements' %>

<% @fnc.observations.each do |obser| %>
  <% if obser.user.entreprise_type=="Etablissement"  %>
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title">Etablissement</h3>
      </div>
      <div class="panel-body">
        <%= obser.message %>
      </div>
      <div class="panel-footer"><%= l(obser.created_at) %> </div>
    </div>

  <% else %>
    <div class="panel panel-success">
      <div class="panel-heading">
        <h3 class="panel-title">Titulaire du march√©</h3>
      </div>
      <div class="panel-body">
        <%= obser.message %>
      </div>
      <div class="panel-footer"><%= l(obser.created_at) %> </div>
    </div>
  <% end %>
<% end %>






